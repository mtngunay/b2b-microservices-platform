{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Information",
        "Microsoft.AspNetCore": "Warning",
        "Microsoft.EntityFrameworkCore": "Warning",
        "System": "Warning",
        "MassTransit": "Debug",
        "B2B": "Debug"
      }
    },
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "outputTemplate": "[{Timestamp:HH:mm:ss} {Level:u3}] {CorrelationId} {Message:lj}{NewLine}{Exception}"
        }
      }
    ]
  },
  "Elasticsearch": {
    "Uri": "http://localhost:9200",
    "IndexFormat": "b2b-api-logs-dev-{0:yyyy.MM.dd}"
  },
  "ConnectionStrings": {
    "WriteDb": "Server=localhost;Database=B2B_Dev;User Id=sa;Password=YourPassword123!;TrustServerCertificate=True",
    "ReadDb": "mongodb://localhost:27017/B2B_Dev",
    "Redis": "localhost:6379",
    "RabbitMq": "amqp://guest:guest@localhost:5672"
  },
  "OpenTelemetry": {
    "ServiceName": "B2B.API.Dev",
    "JaegerEndpoint": "http://localhost:14268/api/traces"
  }
}
